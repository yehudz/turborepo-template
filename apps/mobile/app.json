{
  "expo": {
    "name": "Turborepo Template",
    "slug": "turborepo-template",
    "version": "1.0.0",
    "scheme": "com.turbo.example",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/d4240921-dde6-4727-a2f5-c8798e038b87"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.turbo.example"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d4240921-dde6-4727-a2f5-c8798e038b87"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
