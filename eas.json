{
  "cli": {
    "version": ">= 16.17.4"
  },
  "build": {
    "qa": {
      "distribution": "internal",
      "channel": "qa",
      "env": {
        "EXPO_PUBLIC_ENV": "qa"
      },
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "buildConfiguration": "Release"
      }
    },
    "production": {
      "distribution": "store",
      "channel": "production",
      "env": {
        "EXPO_PUBLIC_ENV": "production"
      },
      "android": {
        "buildType": "app-bundle"
      },
      "ios": {
        "buildConfiguration": "Release"
      }
    }
  },
  "submit": {
    "qa": {},
    "production": {}
  }
}
